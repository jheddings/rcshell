# setup 'ls'
alias l='/bin/ls -hF'
alias ll='l -l'
alias la='l -a'
alias lla='l -la'

# interactive commands by default...
alias rm='/bin/rm -i'
alias cp='/bin/cp -i'
alias mv='/bin/mv -i'

alias bk='/bin/echo && /bin/echo && /bin/echo ================================================================================'

alias g='gvim'
alias e='vim'

